@Article{blanciforti83b,
  author = {Laura A. Blanciforti and Richard Green},
  title = {An {Almost} {Ideal} {Demand} {System} Incorporating Habits: An
  Analysis of Expenditures on Food and Aggregate Commodity Groups},
  journal = {The Review of Economics and Statistics},
  year = {1983},
  volume = {65},
  number = {3},
  pages = {511-515},
  abstract = {The article analyzes the effectiveness of almost ideal demand
  system (AIDS), a demand system, developed by economists Angus Deaton and John
  Muellbauer. The homogeneity and symmetry restrictions which were rejected by
  Deaton and Muellbauer were tested with the more dynamic generalized AIDS to
  determine if the exclusion of dynamic elements may have accounted for their
  result. Annual United States time series data for the years 1948 to 1978 were
  used to estimate the demand systems. For the 11-aggregate commodity
  classification, the commodities were food, alcohol and tobacco, clothing,
  housing, utilities, transportation, medical care, durable goods, other
  nondurable goods, other services and other miscellaneous goods. The empirical
  results indicated the presence of habits or persistence in consumption
  behavior patterns of consumers when the AIDS is used as the maintained
  hypothesis. Autocorrelation was also found to have a large impact on
  elasticity estimates in the AIDS. The approximate proportionality
  relationship that exists for the additive linear expenditure system was not
  found to exist for the more flexible AIDS.}
}

